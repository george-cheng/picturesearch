.container{

}
.bg{
  position: absolute;
  z-index: -1;
  width: 100vw; 
  height: 100vh;
  filter: blur(5px)
}
.search-view{
    padding: 0 50rpx;
    color: #fff;
}
.logo{
  display: block;
  text-align: center;
  font-size: 30px;
  padding-top: 100rpx;
  padding-bottom: 200rpx;
}
.search-form{
  display: block;
  padding: 3px 5px;
  overflow: hidden; 
  border: 1px solid #fff;
  border-radius: 5px;
  color: #fff;
}
.search-form button{
  float: right;
  margin-left: 5px;
  width: 35px;
  height: 35px;
  background: transparent;
}
.search-form button::after{
  border: none
}
.search-form button image{
  display: block;
  margin-top: 10rpx;
  width: 100%;
  height: 100%;
}
.search-form input{
  height: 35px;
}
.link{
  display: flex;
  flex-wrap: wrap;
  padding-top: 3px;
}
.link navigator{
  text-align: center;
  width: 25%;
  padding: 3px 0;
}